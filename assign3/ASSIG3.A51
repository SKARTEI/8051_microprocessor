	ORG 4000H
	MOV PSW, #08H
;
	MOV DPTR, #5000H
	MOV A, #2
	MOV B, #2
	MOV R0, #8
	MOV R5, #2
;
LOOP1:
	MOV R1, #8
	MOV R3, #8
	MOV A, R5
LOOP2:
	MOVX @DPTR, A
	INC DPTR
	ADD A, B
	DA A
	MOVX @DPTR, A
	DJNZ R1, LOOP2
;
LOOP3:
	INC DPTR
	DJNZ R3, LOOP3
;
	INC A
	INC B
	DJNZ R0, LOOP1
;
	MOV PSW, #0H
	JMP 0057H
;
	END
	