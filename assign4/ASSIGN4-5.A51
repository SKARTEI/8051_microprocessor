	ORG 4000H
	MOV PSW, #08H
;
	MOV DPTR, #5000H
	MOV A, #2
	MOV B, #2
	MOV R6, #8
;
LOOPX:
	CALL MULTI
	INC B
	MOV A, B
	DJNZ R6, LOOPX
;
	MOV PSW, #0H
	JMP 0057H
;
MULTI:
	MOV R7, #8
	MOV R5, #7
	MOVX @DPTR, A
	INC DPTR
LOOP1:
	ADD A, B
	DA A
	MOVX @DPTR, A
	INC DPTR
	DJNZ R7, LOOP1
;
LOOP2:
	INC DPTR
	DJNZ R5, LOOP2
	RET
;
	END
	